<h6><%= @message.product.name %></h6>
<div class="messages-bg">
  <div class="message-container">
    <div class="messages row">
      <% @message.product.messages.each do |message| %>
        <ul>
          <li><%= message.id %></li>
          <li><%= message.title %></li>
          <li> <%= message.content %></li>
        </ul>
      <% end %>
    </div>
  </div>
</div>
<%= link_to "Back to inbox",  messages_path %>