<div class="row">
  <div class="col-xs-12 col-sm-8 offset-sm-2 my-2">
    <h1>Add new license</h1>
    <%= simple_form_for @license, url: suppliers_licenses_path  do |f| %>
      <div class="form-inputs">
        <%= f.input :number %>
        <%= f.input :authority %>
      </div>
      <div class="form-actions">
        <%= f.button :submit %>
        <%= link_to "Back to product index", products_path %>
      </div>
    <% end %>
  </div>
</div>